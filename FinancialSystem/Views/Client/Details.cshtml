@model Model.Entity.Client
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>
<div class="panel panel-default">
    <div class="panel-heading">Personal Information</div>
    <div class="panel-body">
        <dl class="dl-horizontal">
            <dt>Code</dt>
            <dd>@Model.idClient</dd>

            <dt>Name</dt>
            <dd>@Model.name</dd>


            <dt>PPS</dt>
            <dd>@Model.pps</dd>

            <dt>Address</dt>
            <dd>@Model.address</dd>

            <dt>Phone</dt>
            <dd>@Model.phone</dd>
        </dl>
    </div>     
        @Html.ActionLink("Back to list", "Index", new { }, new { @class="form-actions btn btn-primary"})
    
</div>
